{
  "excelPaths": [
    "../0_no_loc/data",
    "data"
  ],
  "validation": {
    "searchFileRoot": "../../Client/Resources",
    "sheetNameReg": "^[A-Z][a-zA-Z0-9]*$",
    "colNameReg": "^[A-Z][a-zA-Z0-9_]*$",
    "enumNameReg": "^E[A-Z][A-Za-z0-9_]*$",
    "enumFieldNameReg": "[A-Z][a-zA-Z0-9_]*$"
  },
  "tableDataRule": {
    "nameRowIndex": 1,
    "typeRowIndex": 2,
    "descRowIndex": 3,
    "dataStartRowIndex": 4,
    "calculateFormula": false,
    "emptyPlaceholder": null,
    "separatorTuple": "|",
    "separatorList": ";"
  },
  "localization": {
    "enable": true,
    "sheetName": "Loc",
    "defaultLang": "zh-Hans",
    "useHashId": true,
    "autoGenKey": false,
    "checkKeyExist": true
  },
  "exportCommon": {
